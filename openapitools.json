{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.3.0",
    "generators": {
      "default": {
        "generatorName": "typescript-fetch",
        "output": "./.generated/",
        "inputSpec": "./src/schema/swagger.yaml",
        "additionalProperties": {
          "supportsES6": true,
          "withInterfaces": true
        }
      }
    }
  }
}
